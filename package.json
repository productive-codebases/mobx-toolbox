{
  "name": "@productive-codebases/mobx-toolbox",
  "description": "MobX tooling for productive codebases.",
  "version": "0.1.0-beta.28",
  "scripts": {
    ":d": "npm run dev",
    "build": "CI=1 vite build",
    "check:code": "tsc --noEmit",
    "dev": "vite",
    "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "prepare": "husky install",
    "prepublishOnly": "npm run check:code && npm t && npm run build",
    "preview": "vite preview",
    "test": "vitest --run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "npm run format"
    ]
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/mobx-toolbox.es.js",
      "require": "./dist/mobx-toolbox.umd.js"
    }
  },
  "main": "./dist/mobx-toolbox.cjs.js",
  "module": "./dist/mobx-toolbox.es.js",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@productive-codebases/toolbox": "^1.1.0-beta.8",
    "@types/lodash.escaperegexp": "^4.1.7",
    "@types/node": "^18.15.13",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.2.1",
    "lodash.escaperegexp": "^4.1.2",
    "mobx": "^6.9.0",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.1",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-linter": "^2.0.2",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.30.1"
  },
  "peerDependencies": {
    "@productive-codebases/toolbox": ">=1.1.0-beta.13",
    "lodash.escaperegexp": ">=4.x",
    "mobx": ">=6.8.0",
    "react": ">=18.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/productive-codebases/mobx-toolbox"
  },
  "keywords": [],
  "author": "Productive Codebases",
  "bugs": {
    "url": "https://github.com/productive-codebases/mobx-toolbox/issues"
  },
  "homepage": "https://github.com/productive-codebases/mobx-toolbox#readme",
  "license": "MIT"
}
