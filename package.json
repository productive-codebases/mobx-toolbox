{
  "name": "@productive-codebases/mobx-toolbox",
  "description": "MobX tooling for productive codebases.",
  "version": "0.1.0-beta.34",
  "scripts": {
    ":d": "npm run dev",
    "build": "CI=1 vite build",
    "check:code": "tsc --noEmit",
    "dev": "vite",
    "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "prepare": "husky install",
    "prepublishOnly": "npm run check:code && npm t && npm run build",
    "preview": "vite preview",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "npm run format"
    ]
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/mobx-toolbox.es.js",
      "require": "./dist/mobx-toolbox.cjs.js"
    }
  },
  "main": "./dist/mobx-toolbox.cjs.js",
  "module": "./dist/mobx-toolbox.es.js",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@productive-codebases/toolbox": "^1.1.0-beta.8",
    "@types/jest": "^29.5.4",
    "@types/lodash.escaperegexp": "^4.1.7",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "lodash.escaperegexp": "^4.1.2",
    "mobx": "^6.10.2",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.3",
    "vite-plugin-linter": "^2.0.5",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "peerDependencies": {
    "@productive-codebases/toolbox": ">=1.1.0-beta.13",
    "lodash.escaperegexp": ">=4.x",
    "mobx": ">=6.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/productive-codebases/mobx-toolbox"
  },
  "keywords": [],
  "author": "Productive Codebases",
  "bugs": {
    "url": "https://github.com/productive-codebases/mobx-toolbox/issues"
  },
  "homepage": "https://github.com/productive-codebases/mobx-toolbox#readme",
  "license": "MIT"
}
